{
  "name": "confusion",
  "version": "0.1.0",
  "description": "confusion website made with boostrap",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "lite": "lite-server",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "author": "Dang Tran Nam",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangtrannam/Confusion.git"
  },
  "bugs": {
    "url": "https://github.com/dangtrannam/Confusion/issues"
  },
  "homepage": "https://github.com/dangtrannam/Confusion#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "parallelshell": "^3.0.2"
  }
}
